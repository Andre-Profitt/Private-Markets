# Search Service Configuration

# Server
SEARCH_SERVICE_PORT=3011
NODE_ENV=development

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/search?schema=public"

# CORS
CORS_ORIGIN=http://localhost:3000

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=search-service
KAFKA_GROUP_ID=search-group

# Redis (for caching)
REDIS_HOST=localhost
REDIS_PORT=6379

# Logging
LOG_LEVEL=debug
